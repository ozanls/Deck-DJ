@model AudioApplication.Models.CategoryDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Confirm Delete</h2>

<p> Are you sure you want to delete <i>@Model.CategoryName</i>?</p>

<form method="POST" action="/Category/Delete/@Model.CategoryId">

    <input type="submit" value="Confirm Delete" />

</form>